<?xml version="1.0"?>
<launch>
	<arg name="hz" default="10"/>
	<arg name="max_speed" default="1.0"/>
	<arg name="max_yawrate" default="1.0"/>
	<arg name="model_path" default="/home/amsl/models/libtorch_model/ppo_model.pt"/>

    <node pkg="motion_planner_with_drl" type="localmap_scan" name="localmap_scan">
		<remap from="local_map" to="local_costmap"/>
	</node>
    <!-- <node name="mpdrl" pkg="motion_planner_with_drl" type="motion_planner_with_drl.py"> -->
	<!-- 	<remap from="/cmd_vel" to="/local_path/cmd_vel"/> -->
	<node name="test_navigator" pkg="motion_planner_with_drl" type="test_navigator"/>
    <node name="mpdrl" pkg="motion_planner_with_drl" type="motion_planner_with_drl">
		<param name="HZ" value="$(arg hz)" type="int"/>
		<param name="MAX_SPEED" value="$(arg max_speed)" type="double"/>
		<param name="MAX_YAWRATE" value="$(arg max_yawrate)" type="double"/>
		<param name="MODEL_PATH" value="$(arg model_path)" type="str"/>
		<remap from="/local_path/cmd_vel" to="/velocity"/>
	</node>
</launch>
